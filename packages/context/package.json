{
  "name": "@corpuscule/context",
  "version": "0.14.2",
  "description": "DOM-dependent context for Corpuscule",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build:remove && run-p build:babel build:dts",
    "build:babel": "babel src -d lib -x .ts --config-file ../../.babelrc",
    "build:dts": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:remove": "rimraf lib",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "author": "Vlad Rindevich <rindevich.vs@gmail.com> (https://github.com/Lodin)",
  "bugs": {
    "url": "https://github.com/corpusculejs/corpuscule/issues"
  },
  "homepage": "https://github.com/corpusculejs/corpuscule/packages/context#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corpusculejs/corpuscule.git"
  },
  "dependencies": {
    "@corpuscule/typings": "^0.14.0",
    "@corpuscule/utils": "^0.14.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
