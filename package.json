{
  "name": "corpuscule",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --parallel",
    "docs": "typedoc --tsconfig ../../tsconfig.json packages && node scripts/fixDocs.js",
    "lint": "run-p lint:es lint:ts",
    "lint-staged": "lint-staged",
    "lint:es": "eslint packages/**/*.(ts|js)",
    "lint:ts": "run-p tslint typecheck",
    "tslint": "tslint \"packages/**/*.ts\" -e \"packages/**/+(node_modules|lib)/**/*\" -c tslint.json -p tsconfig.json -t verbose",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "postinstall": "run-s bootstrap",
    "prepublishOnly": "run-s build lint test",
    "prettier": "prettier --write \"{packages,scripts,test}/**/*.{js,ts}\"",
    "test": "karma start test/karma.conf.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corpusculejs/corpuscule.git"
  },
  "author": "Vlad Rindevich <rindevich.vs@gmail.com> (https://github.com/Lodin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corpusculejs/corpuscule/issues"
  },
  "homepage": "https://github.com/corpusculejs/corpuscule#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@corpuscule/babel-preset": "^0.2.2",
    "@corpuscule/custom-builtin-elements": "^0.3.0",
    "@open-wc/karma-esm": "^2.12.8",
    "@open-wc/testing-helpers": "^1.5.1",
    "@types/jasmine": "^3.5.2",
    "@types/uuid": "^3.4.7",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-poetez": "^0.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "final-form": "^4.18.7",
    "husky": "^4.2.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.5.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.0",
    "karma-safarinative-launcher": "^1.1.0",
    "karma-webpack": "^4.0.2",
    "lerna": "^3.20.2",
    "lint-staged": "^10.0.6",
    "lit-html": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "redux": "^4.0.5",
    "rimraf": "^3.0.1",
    "rollup": "^1.30.1",
    "rollup-plugin-babel": "^4.3.3",
    "storeon": "^0.9.8",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "tslint-config-poetez": "^2.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typedoc": "^0.16.9",
    "typedoc-plugin-external-module-name": "^3.0.0",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typescript": "^3.7.5",
    "universal-router": "^8.3.0",
    "uuid": "^3.4.0",
    "webpack": "^4.41.5"
  }
}
