{
  "extends": "eslint-config-poetez",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "callback-return": "off",
    "global-require": "off",
    "no-unused-vars": ["error", {
      "args": "all",
      "argsIgnorePattern": "^_[a-zA-Z]*$",
      "caughtErrors": "all",
      "caughtErrorsIgnorePattern": "^_[a-zA-Z]*$",
      "vars": "all",
      "varsIgnorePattern": "^_[a-zA-Z]*$"
    }],
    "symbol-description": "off"
  }
}
