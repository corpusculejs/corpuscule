{
  "name": "corpuscule",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build --parallel",
    "lint": "run-p lint:es lint:ts",
    "lint-staged": "lint-staged",
    "lint:es": "eslint packages/**/*.js",
    "lint:ts": "run-p tslint typecheck",
    "tslint": "tslint \"packages/**/*.ts\" -e \"packages/**/+(node_modules|lib)/**/*\" -c tslint.json -p tsconfig.json -t verbose",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepare": "run-s bootstrap build lint",
    "prepublish": "run-s build",
    "prettier": "prettier --write \"{packages,scripts,test}/**/*.{js,ts}\"",
    "test": "karma start test/karma.conf.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corpusculejs/corpuscule.git"
  },
  "author": "Vlad Rindevich <rindevich.vs@gmail.com> (https://github.com/Lodin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corpusculejs/corpuscule/issues"
  },
  "homepage": "https://github.com/corpusculejs/corpuscule#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@corpuscule/babel-preset": "^0.2.2",
    "@open-wc/testing-helpers": "^0.9.5",
    "@types/jasmine": "^3.3.13",
    "@types/uuid": "^3.4.4",
    "babel-loader": "^8.0.6",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-poetez": "^0.1.3",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "final-form": "^4.13.1",
    "husky": "^2.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.4.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^3.0.5",
    "lerna": "^3.14.1",
    "lint-staged": "^8.1.7",
    "lit-html": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.0",
    "rollup-plugin-babel": "^4.3.2",
    "storeon": "^0.8.3",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-config-poetez": "^1.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.1",
    "universal-router": "^8.2.0",
    "uuid": "^3.3.2",
    "webpack": "^4.33.0"
  }
}
