{
  "name": "@corpuscule/styles",
  "version": "0.14.2",
  "description": "Styling utils for Corpuscule",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build:remove && npm run build:babel",
    "build:babel": "babel src -d lib -x .ts,.js --ignore src/*.d.ts -D",
    "build:remove": "rimraf lib",
    "test": "karma start ../../test/karma.conf.js",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "author": "Vlad Rindevich <rindevich.vs@gmail.com> (https://github.com/Lodin)",
  "bugs": {
    "url": "https://github.com/corpusculejs/corpuscule/issues"
  },
  "homepage": "https://github.com/corpusculejs/corpuscule/packages/styles#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corpusculejs/corpuscule.git"
  },
  "dependencies": {
    "@corpuscule/utils": "^0.14.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
