{
  "comments": false,
  "presets": ["@babel/preset-typescript"],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    [
      "@babel/plugin-proposal-nullish-coalescing-operator",
      {
        "loose": true
      }
    ],
    ["@babel/plugin-proposal-optional-chaining", {"loose": true}]
  ]
}
