{
  "extends": ["tslint-config-poetez", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "max-classes-per-file": false,
    "no-implicit-dependencies": false,
    "no-redundant-jsdoc": false,
    "prefer-for-of": false,
    "prettier": [true, ".prettierrc"]
  }
}
